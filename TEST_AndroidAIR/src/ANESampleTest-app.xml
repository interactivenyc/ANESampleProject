<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/3.8">

	<id>ANESampleTest</id>
	<filename>ANESampleTest</filename>
	<name>ANESampleTest</name>
	<versionNumber>0.0.0</versionNumber>
	<initialWindow>
		<content>ANESampleTest.swf</content>
		<aspectRatio>landscape</aspectRatio>
		<autoOrients>true</autoOrients>
		<fullScreen>true</fullScreen>
		<visible>true</visible>
	</initialWindow>
	<android>
		<manifestAdditions><![CDATA[
			<manifest android:installLocation="auto">
			    <uses-permission android:name="android.permission.INTERNET"/>
			    
			    <application android:enabled="true" android:persistent="true"/>
			    
			    
				
			</manifest>
			
		]]></manifestAdditions>
	</android>
	<iPhone>
		<InfoAdditions><![CDATA[
			<key>UIDeviceFamily</key>
			<array>
				<string>1</string>
				<string>2</string>
			</array>
		]]></InfoAdditions>
		<requestedDisplayResolution>high</requestedDisplayResolution>
	</iPhone>
	
<extensions>
    <extensionID>com.adobe.sampleasextension</extensionID>
    </extensions>
</application>


